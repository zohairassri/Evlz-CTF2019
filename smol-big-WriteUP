flag='evlz{'

#output for 'evlz{'
s='11111011011000101110111001110100000' 

while True :

	for i in range(32,127):

		data=os.popen("  python -c 'print chr( {} ) ' |  ./smol-big   ".format(i)).readlines()
		k=data[-1][:-1]		
		if str(s)+str(k) in liste :
			s=s+data[-1][:-1]
			flag=flag+chr(i)
			print flag

	if flag[-3:] == "ct'f":
		break

print flag
